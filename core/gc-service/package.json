    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-chai-friendly": "^0.6.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-security": "^1.4.0",
    "mocha": "^7.1.1",
    "mockery": "^2.1.0",
    "nock": "^13.1.0",
    "nyc": "^15.1.0",
    "sinon": "^11.1.1",
    "uuid": "^3.3.3"
