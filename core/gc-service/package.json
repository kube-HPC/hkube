    "@hkube/consts": "^1.0.35",
    "@hkube/db": "^1.0.40",
    "@hkube/etcd": "^5.1.6",
    "@hkube/healthchecks": "^1.0.2",
    "@hkube/kubernetes-client": "^1.0.35",
    "@hkube/logger": "^1.0.30",
    "@hkube/redis-utils": "^1.0.3",
    "@hkube/rest-server": "^1.0.17",
    "@hkube/storage-manager": "^2.0.33",
    "cron": "^1.8.2",
    "cronstrue": "^1.114.0",
    "fs-extra": "^10.0.0",
    "glob": "^7.1.7",
    "moment": "^2.29.1",
    "requestretry": "^5.0.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "coveralls": "^3.1.0",
