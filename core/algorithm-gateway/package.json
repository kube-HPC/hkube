{
    "name": "algorithm-gateway",
    "version": "2.2.1",
    "description": "algorithm-gateway",
    "main": "app.js",
    "dependencies": {
        "@hkube/config": "^2.0.11",
        "@hkube/logger": "^2.0.1",
        "@hkube/nodejs-wrapper": "^2.0.50",
        "@hkube/rest-server": "^1.0.18"
    },
    "devDependencies": {
        "@hkube/uid": "^1.0.4",
