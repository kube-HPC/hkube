        "nyc": "^14.1.1",
        "pre-commit": "^1.2.2",
        "request": "^2.88.0",
        "sinon": "^7.4.1"
    },
    "scripts": {
        "lint": "./node_modules/eslint/bin/eslint.js .",
        "test": "NODE_ENV=test ./node_modules/mocha/bin/_mocha tests --colors --recursive --exit",
        "test:cov": "NODE_ENV=test ./node_modules/nyc/bin/nyc.js --reporter=html --reporter=text --reporter=lcov mocha tests --colors --recursive --exit",
        "coverage": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
        "start": "node app",
        "build": "./dockerfile/build.sh api-server",
        "build-swagger": "node ./api/rest-api/swagger-builder.js"
    },
    "pre-commit": {
        "run": [
            "lint",
            "test"
        ]
    },
    "nyc": {
        "exclude": [
            "bootstrap.js",
            "tests",
            "lib/producer"
        ]
    },
