    {
        "name": "batch_trigger",
        "nodes": [
            {
                "nodeName": "count",
                "algorithmName": "eval-alg",
                "input": [
                    "@flowInput"
                ],
                "extraData": {
                    "code": [
                        "function count(input) {",
                        "let dict = {}",
                        "input[0].forEach(r => {",
                        "Object.entries(r).forEach(([k, v]) => {",
                        "if (!dict[k]) {",
                        "dict[k] = 0;",
                        "}",
                        "dict[k] += v",
                        "});",
                        "});",
                        "return dict",
                        "}"
                    ]
                }
            }
        ],
        "flowInput": {},
        "options": {
            "batchTolerance": 100,
            "progressVerbosityLevel": "debug"
        }
