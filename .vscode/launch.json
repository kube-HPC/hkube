    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "pack",
            "program": "${workspaceFolder}/bin/hkube",
            "args": [
                "pack",
                "-s"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "load",
            "program": "${workspaceFolder}/bin/hkube",
            "args": [
                "load",
                "--folder=/home/yehil/.hkube/1.0.217",
                "-t"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "deploy",
            "program": "${workspaceFolder}/bin/hkube",
            "args": [
                "deploy",
                "-t"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "clone",
            "program": "${workspaceFolder}/bin/hkube",
            "args": [
                "c",
                "-a"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "minkube",
            "program": "${workspaceFolder}/bin/hkube",
            "args": [
                "--kubeConfig=~/.kube/admin.conf",
                "m",
                "-t"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "npm",
            "program": "${workspaceFolder}/bin/hkube",
            "args": [
                "npm",
                "-d"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch repos",
            "program": "${workspaceFolder}/repos.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch pull",
            "program": "${workspaceFolder}/bin/hkube",
            "args": [
                "p",
                "-d"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch kuberApi",
            "program": "${workspaceFolder}/minikube/kubernetes-api.js",
            "args": [
                "cloneWithNpm",
                "--dev"
            ]
        }
    ]
}